net S7_out[1]	loc = E6;
net S7_out[2]	loc = C5;
net S7_out[3]	loc = F7;
net S7_out[4]	loc = D6;
net S7_out[5]	loc = E7;
net S7_out[6]	loc = D5;
net S7_out[7]	loc = C6;

net Dig_out[0]	loc = D8;
net Dig_out[1]	loc = E8;
net Dig_out[2]	loc = F9;
net Dig_out[3]	loc = F10;
net Dig_out[4]	loc = E10;
net Dig_out[5]	loc = D9;

